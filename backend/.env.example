# Face Attendance Web Backend Configuration
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=5000

# ImageKit Configuration (Optional - for cloud image storage)
# If not provided, images will be stored locally only
IMAGEKIT_PRIVATE_KEY=your_private_key_here
IMAGEKIT_PUBLIC_KEY=your_public_key_here
IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_endpoint_here/

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=false